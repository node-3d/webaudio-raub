{
	"author": "Luis Blanco <luisblanco1337@gmail.com>",
	"name": "webaudio-raub",
	"version": "5.1.1",
	"description": "Web Audio for Node.js",
	"license": "MIT",
	"main": "index.js",
	"keywords": [
		"addon",
		"webaudio",
		"labsound",
		"audio",
		"sound",
		"c++",
		"bindings",
		"native",
		"gyp"
	],
	"files": [
		"js",
		"core.js",
		"index.js",
		"index.d.ts",
		"install.js",
		"LICENSE",
		"package.json",
		"README.md"
	],
	"engines": {
		"node": ">=22.14.0",
		"npm": ">=10.9.2"
	},
	"scripts": {
		"postinstall": "node install",
		"eslint": "eslint .",
		"build": "cd src && node-gyp rebuild -j max --silent && node -e \"require('addon-tools-raub').cpbin('webaudio')\"",
		"build-only": "cd src && node-gyp build -j max --silent && node -e \"require('addon-tools-raub').cpbin('webaudio')\"",
		"test": "node --test"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/node-3d/webaudio-raub.git"
	},
	"dependencies": {
		"addon-tools-raub": "^9.3.0",
		"deps-labsound-raub": "^6.0.0",
		"segfault-raub": "^3.2.0"
	},
	"devDependencies": {
		"@types/node": "^22.13.4",
		"@eslint/js": "^9.20.0",
		"eslint": "^9.20.1",
		"node-addon-api": "^8.3.0",
		"typescript": "^5.7.3"
	}
}
